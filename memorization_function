// ### Memorization Function

// Create a memorization function that can be used to memoize any function.

```javascript
// memorize.js

const memorize = (fn) => {
  const cache = {};

  return (...args) => {
    const key = JSON.stringify(args);

    if (cache[key]) {
      return cache[key];
    }

    const result = fn(...args);
    cache[key] = result;
    return result;
  };
};

export default memorize;
